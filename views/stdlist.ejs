<body>
    <div class="container">
        <h3>Student's List (Latest 10 Entries)</h3>
        
        <table class="table table-hover table-bordered">
            <thead>
              <tr>
                <th>Sr. No.</th>
                <th>Name</th>
                <th>Batch</th>
                <th>College</th>
                <th>Status</th>
                <th colspan="3">Scores</th>
              </tr>
              <tr>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
                <th>DSA</th>
                <th>Web Development</th>
                <th>React</th>
              </tr>
            </thead>
            <tbody>
              <% stdlist.forEach(function(student, index) { %>
              <tr>
                <td><%= index + 1 %></td> <!-- add 1 to start from 1 instead of 0 -->
                <td><%= student.name === null || student.name === undefined ? "guest" : student.name %></td>
                <td><%= student.batch %></td>
                <td><%= student.college %></td>
                <!-- If placed then using colour green, else red -->
                <td><span style='color: <%= student.status ? "green" : "red" %>'><%= student.status ? "Placed" : "Not Placed" %></span></td>
                <td><%= student.dsa %></td>
                <td><%= student.webd %></td>
                <td><%= student.react %></td>
              </tr>
              <% }) %>
            </tbody>
          </table>
          
          <h2>We Did not Prodvide Company Details Here...</h2>
          
    </div>
</body>